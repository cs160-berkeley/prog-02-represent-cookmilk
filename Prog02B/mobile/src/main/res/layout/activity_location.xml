<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.cookmilk.prog02b.LocationActivity"
    android:weightSum="1">

    <Space
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.08" />

    <TextView
        android:layout_width="337dp"
        android:layout_height="wrap_content"
        android:text="Result of Current Location"
        android:id="@+id/textView4"
        android:layout_gravity="center_horizontal"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#030303"
        android:textSize="20dp"
        android:background="#d8d8d8" />

    <Space
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.04" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senators"
        android:id="@+id/textView5"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.03"
        android:textColor="#3fd5fa"
        android:textStyle="bold"
        android:textSize="18dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/zipImgBut1"
            android:layout_alignParentStart="true"
            android:src="@mipmap/hillarycli"
            android:background="#00ffffff"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name: First Last
Party: Democratic
Email: firstlast@email.com
Website: helloWord.com
Latest Twitter: Hello World!"
            android:id="@+id/textView7"
            android:textSize="12dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/zipImgBut1"
            android:layout_marginStart="39dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_gravity="center_horizontal">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/zipImgbut2"
            android:layout_alignParentStart="true"
            android:src="@mipmap/hillarycli"
            android:background="#00ffffff"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name: First Last
Party: Democratic
Email: firstlast@email.com
Website: helloWord.com
Latest Twitter: Hello World!"
            android:id="@+id/textView6"
            android:textSize="12dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/zipImgbut2"
            android:layout_marginStart="39dp" />
    </RelativeLayout>

    <Space
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.08" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Representative"
        android:id="@+id/textView8"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.03"
        android:textColor="#fa3fc2"
        android:textStyle="bold"
        android:textSize="18dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_gravity="center_horizontal">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/zipMoreImgbut1"
            android:layout_alignParentStart="true"
            android:src="@mipmap/bushgeo"
            android:background="#00ffffff"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name: First Last
Party: Democratic
Email: firstlast@email.com
Website: helloWord.com
Latest Twitter: Hello World!"
            android:id="@+id/textView9"
            android:textSize="12dp"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/zipMoreImgbut1"
            android:layout_marginStart="39dp" />
    </RelativeLayout>

    <Space
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.17" />

</LinearLayout>
